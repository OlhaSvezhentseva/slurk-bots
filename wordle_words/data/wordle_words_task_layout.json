{
    "title": "Wordle Game Bot",
    "html": [
    {
      "layout-type": "div",
      "id": "typing",
      "style": "margin: 5px 10px; min-height: 20px"
    },
    {
      "layout-type": "div",
      "class": "card",
      "layout-content": [
        {
          "layout-type": "div",
          "layout-content": [
            {
              "layout-type": "b",
              "layout-content": [
                {
                  "layout-type": "lighter",
                  "id": "instr_title",
                  "style": "min-height: 20px"
                }
              ]
            }
          ],
          "style": "color: #000000;"
        },
        {
          "layout-type": "hr",
          "layout-content": "",
          "style": "margin: 2px; background-color: #cfe2f3; opacity: .50;"
        },
        {
          "layout-type": "div",
          "id": "instr",
          "style": "white-space: pre-wrap; background-color: #cfe2f3; "
        },
        {
          "layout-type": "div",
          "id": "text_to_modify",
          "layout-content": "TEXT TO MODIFY",
          "style": "background-color: #cfe2f3;"
        }


      ]
    }
  ],
    "css": {
    ".card": {
      "position": "relative",
      "display": "flex",
      "flex-direction": "column",
      "min-width": "0",
      "background-color": "#cfe2f3;",
      "background-clip": "border-box",
      "border": "1px solid rgba(0,0,0,.125)",
      "border-radius": ".25rem",
      "height": "100vh!important",
      "margin": "0px 15px",
      "padding": "20px"
    },
    "time:before": {
      "content": "''"
    },
    "#image-area": {
      "margin": "0 auto"
    }
  },
    "scripts": {
        "incoming-text": "markdown",
        "submit-message": "send-message",
        "print-history": ["attribute-history", "markdown-history"],
        "typing-users": "typing-users",
        "plain":["wordle_words"]
    },
    "show_latency": false
}